                </li>
                <!-- user login dropdown end -->

            </ul>
            <!-- notificatoin dropdown end-->

        </div>

    </header>
    <!--header end-->
    <!--sidebar start-->
    <section id="container" class="">

        <!--header end-->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse " tabindex="5000" style="overflow: hidden; outline: none;">
                <!-- sidebar menu start-->
                <ul class="sidebar-menu">

                    <li class="sub-menu">
                        <a href="javascript:;" class="">

                            <img alt="" src="~/Content/img/set2.png" />
                            <span>Données</span>

                        </a>
                        <ul class="sub">
                            <li><a class="" href="~/Users/Index"><img alt="" src="~/Content/img/iconfinder_Users_85409.png" /> Personnes</a></li>
              
                        </ul>
                    </li>

                  



                    @*<li class="sub-menu">
                        <a href="~/Report/Index" class="">
                            <img alt="" src="~/Content/img/blue_repicthousebase_1484336386-1.png">
                            <span>
                                Reports
                            </span>
                        </a>
                       
                    </li>*@

                    @*<li class="sub-menu">
                        <a href="~/Export/Index" class="">
                            <img alt="" src="~/Content/img/list.png">
                            <span>
                                Export Customer
                            </span>
                        </a>

                    </li>*@
