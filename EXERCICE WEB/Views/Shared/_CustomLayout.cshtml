                    <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                        <span class="profile-ava">
                            <img alt="" src="~/Content/img/users.png">
                        </span>
                        @*<span class="username">
                            @if (Session["UserID"]!= null)
                            {
                                @User.Identity.Name
                                @Session["UserName"].ToString();
